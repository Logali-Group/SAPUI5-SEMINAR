<mvc:View controllerName="seminar.controller.Launchpad"
            xmlns:mvc="sap.ui.core.mvc" 
            xmlns="sap.m"
            xmlns:uxap="sap.uxap"
            xmlns:sb="seminar.sharedblocks">

    <uxap:ObjectPageLayout>

        <uxap:sections>
                <uxap:ObjectPageSection title="{i18n>LaunchpadModuleA}">
                    <uxap:subSections>
                        <uxap:ObjectPageSubSection>
                            <uxap:blocks>
                                <sb:TileEmployees/>
                            </uxap:blocks>
                        </uxap:ObjectPageSubSection>
                    </uxap:subSections>
                </uxap:ObjectPageSection>

                <uxap:ObjectPageSection title="{i18n>LaunchpadModuleB}">
                    <uxap:subSections>
                        <uxap:ObjectPageSubSection>
                            <uxap:blocks>
                                <sb:TileDeploy/>
                            </uxap:blocks>
                        </uxap:ObjectPageSubSection>
                    </uxap:subSections>
                </uxap:ObjectPageSection>
        </uxap:sections>

    </uxap:ObjectPageLayout>
</mvc:View>
